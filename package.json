{
  "name": "paginas-comercio-local",
  "version": "0.1.0",
  "private": true,
  "description": "Plataforma Multi-Tenant para Páginas de Comércio Local",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "setup": "powershell -ExecutionPolicy Bypass -File ./setup.ps1",
    "setup:skip-install": "powershell -ExecutionPolicy Bypass -File ./setup.ps1 -SkipInstall",
    "setup:with-seed": "powershell -ExecutionPolicy Bypass -File ./setup.ps1 -WithSeed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:seed": "ts-node --transpile-only db/prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:push": "prisma db push",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:api": "jest --testPathPattern='.test.ts$'",
    "health": "curl http://localhost:3000/api/health",
    "clean": "rm -rf .next dist coverage node_modules",
    "ci": "npm run lint && npm run type-check && npm run test && npm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.8.0",
    "@sentry/nextjs": "^7.80.0",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.0.0",
    "next": "^14.1.0",
    "next-auth": "^4.24.13",
    "pino": "^10.1.0",
    "pino-pretty": "^10.3.0",
    "postcss": "^8.4.32",
    "prisma": "^5.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.0",
    "sharp": "^0.32.0",
    "stripe": "^14.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "^16.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "prisma": {
    "schema": "db/prisma/schema.prisma"
  }
}
